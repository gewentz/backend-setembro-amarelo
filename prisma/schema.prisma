generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Resposta {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  q1      String
  q2      String
  q3      String
  q4      String
  q5      String
  q6      String
  q7      String
  q8      String
  q9      String
  q10     String
  q11     String
  q12     String
  q13     String
  q14     String
  q15     String
}
